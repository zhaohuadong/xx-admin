    "js-cookie": "3.0.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "nprogress": "0.2.0",
    "pinia": "2.0.22",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "3.2.45",
    "vue-router": "4.1.4"
