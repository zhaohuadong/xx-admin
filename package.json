    "unplugin-auto-import": "0.11.4",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.22.12",
    "vite": "3.2.3",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-vue-setup-extend": "0.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
